{
  "name"           : "stubapi-node",
  "author"         : "WhiteTrefoil <whitetrefoil@gmail.com>",
  "version"        : "0.1.0",
  "description"    : "A mock server to help web page development.",
  "private"        : true,
  "main"           : "lib/index.js",
  "homepage"       : "https://github.com/whitetrefoil/stubapi-node#readme",
  "repository"     : {
    "type": "git",
    "url" : "git+https://github.com/whitetrefoil/stubapi-node.git"
  },
  "bugs"           : {
    "url": "https://github.com/whitetrefoil/stubapi-node/issues"
  },
  "scripts"        : {
    "build"   : "rimraf lib && cross-env NODE_ENV=production babel --out-dir lib src",
    "watch"   : "rimraf lib && cross-env NODE_ENV=development babel -s both --out-dir lib --watch src",
    "gulp"    : "gulp",
    "serve"   : "cross-env DEBUG=koa* NODE_ENV=development node $NODE_DEBUG_OPTION ./lib/index.js",
    "test"    : "cross-env NODE_ENV=test BABEL_ENV=test mocha --compilers js:babel-register tests/unit/**/*.spec.js",
    "coverage": "cross-env NODE_ENV=test BABEL_ENV=test nyc --cache ./node_modules/.bin/_mocha tests/unit/**/*.spec.js",
    "nyan"    : "cross-env NODE_ENV=test BABEL_ENV=test nyc --cache ./node_modules/.bin/_mocha --reporter nyan tests/unit/**/*.spec.js"
  },
  "dependencies"   : {
    "bluebird"             : "^3.4.3",
    "bunyan"               : "^1.8.1",
    "chalk"                : "^1.1.3",
    "connect"              : "^3.4.1",
    "fs-extra"             : "^0.30.0",
    "koa"                  : "=2.0.0-alpha.6",
    "koa-bodyparser"       : "^3.2.0",
    "koa-context-validator": "^0.2.0",
    "koa-etag"             : "^3.0.0",
    "koa-helmet"           : "^2.0.0",
    "koa-router"           : "^7.0.1",
    "lodash"               : "^4.15.0",
    "meow"                 : "^3.7.0",
    "minimist"             : "^1.2.0",
    "require-new"          : "^1.1.0",
    "rimraf"               : "^2.5.4"
  },
  "devDependencies": {
    "babel-cli"                                    : "^6.14.0",
    "babel-eslint"                                 : "^6.1.2",
    "babel-plugin-syntax-trailing-function-commas" : "^6.13.0",
    "babel-plugin-transform-async-to-module-method": "^6.8.0",
    "babel-plugin-transform-class-properties"      : "^6.11.5",
    "babel-preset-es2015-node6"                    : "^0.3.0",
    "chai"                                         : "^3.5.0",
    "cross-env"                                    : "^2.0.0",
    "cucumber"                                     : "^1.2.2",
    "del"                                          : "^2.2.2",
    "eslint"                                       : "^3.3.1",
    "eslint-plugin-babel"                          : "^3.3.0",
    "gulp"                                         : "^3.9.1",
    "gulp-babel"                                   : "^6.1.2",
    "gulp-babel-istanbul"                          : "^1.4.0",
    "gulp-eslint"                                  : "^3.0.1",
    "gulp-header"                                  : "^1.8.8",
    "gulp-if"                                      : "^2.0.1",
    "gulp-inject-modules"                          : "^1.0.0",
    "gulp-istanbul"                                : "^1.1.1",
    "gulp-mocha"                                   : "^3.0.1",
    "gulp-plumber"                                 : "^1.1.0",
    "gulp-sourcemaps"                              : "^1.6.0",
    "gulp-util"                                    : "^3.0.7",
    "mocha"                                        : "^3.0.2",
    "sinon"                                        : "^1.17.5",
    "sinon-as-promised"                            : "^4.0.2",
    "sinon-chai"                                   : "^2.8.0",
    "source-map-support"                           : "^0.4.2",
    "through2"                                     : "^2.0.1",
    "xmlbuilder"                                   : "^8.2.2"
  },
  "license"        : "ISC",
  "engines"        : {
    "node": ">=6.4.0"
  }
}
